{
  "name": "better-chatgpt",
  "private": true,
  "version": "1.0.3",
  "type": "module",
  "homepage": "./",
  "main": "electron/index.cjs",
  "author": "Jing Hua <betterchatgpt@mail.tjh.sg>",
  "description": "Play and chat smarter with BetterChatGPT - an amazing open-source web app with a better UI for exploring OpenAI's ChatGPT API!",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron": "concurrently -k \"BROWSER=none yarn dev\" \"wait-on tcp:5173 && electron .\"",
    "pack": "yarn build && electron-builder --dir",
    "make": "yarn build && electron-builder"
  },
  "build": {
    "appId": "QnA3",
    "productName": "QnA3",
    "artifactName": "${os}-${name}-${version}-${arch}.${ext}",
    "directories": {
      "output": "release"
    },
    "dmg": {
      "title": "${productName} ${version}",
      "icon": "dist/ms-icon-150x150.png"
    },
    "mac": {
      "icon": "dist/ms-icon-150x150.png"
    },
    "linux": {
      "target": [
        "tar.gz",
        "AppImage"
      ],
      "category": "Chat",
      "icon": "dist/ms-icon-150x150.png"
    },
    "win": {
      "target": "NSIS",
      "icon": "dist/ms-icon-150x150.png"
    }
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.1.0",
    "@dqbd/tiktoken": "^1.0.2",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@react-oauth/google": "^0.9.0",
    "@sentry/react": "^7.55.0",
    "@tabler/icons-react": "^2.20.0",
    "@types/react-modal": "^3.16.0",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@web3modal/ethereum": "^2.6.2",
    "@web3modal/react": "^2.6.2",
    "axios": "^1.4.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "html2canvas": "^1.4.1",
    "i18next": "^22.4.11",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "jspdf": "^2.5.1",
    "katex": "^0.16.4",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-markdown": "^8.0.5",
    "react-modal": "^3.16.1",
    "react-random-avatars": "^1.3.1",
    "react-router-dom": "^6.11.2",
    "react-scroll-to-bottom": "^4.2.0",
    "react-simple-typewriter": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.1.3",
    "react-typewriter-hook": "^1.0.1",
    "rehype-highlight": "^6.0.0",
    "rehype-katex": "^6.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sort-by": "^1.2.0",
    "uuid": "^9.0.0",
    "viem": "^1.2.7",
    "wagmi": "^1.3.3",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.192",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-scroll-to-bottom": "^4.2.0",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.0.1",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.4",
    "vite": "^4.1.0",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "wait-on": "^7.0.1"
  }
}
